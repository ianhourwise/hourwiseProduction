            <div class="row wrapper border-bottom white-bg page-heading">
                <div class="col-lg-10">
                    <h2>New Job</h2>
                  
                </div>
                <div class="col-lg-2">

                </div>
            </div>
        <div class="wrapper wrapper-content animated fadeInRight">
            <div class="row">
                <div class="col-lg-12">
                    <div class="ibox float-e-margins">

                        <div class="ibox-content">
                            <form method="get" class="form-horizontal">
                                <div class="form-group"><label class="col-sm-2 control-label">Name</label>
                                    <div class="col-sm-10"><input type="text" class="form-control" id="jobName"></div>
                                </div>
							    <div class="form-group"><label class="col-xs-2 control-label">Created By</label>
                                    <div class="col-xs-10"><p class="form-control-static" id="owner" name="<%= user.id %>"><%= user.email %></p></div>
                                </div>
                				<div class="form-group">
                					<label class="col-xs-2 control-label">Assigned To</label>
                    					<div class="col-sm-10 ">
                            					<select data-placeholder="Assign to..." class="col-xs-12 chosen-select" tabindex="2">
                                                    <option value="">Select</option>
                                                    <% _.each(contacts, function(contact) { %>
                                						<option value="<%= contact.id %>"><%= contact.name %></option>
                                                    <% }) %>
                            					</select>
                                                <br><br>
                                                <div class="row" id="contactContainer">
                                                    <!-- contact cards -->
                                                </div>
                    					</div>
                				</div>	   
                				<div class="hr-line-dashed"></div>
                                <div class="form-group"><label class="col-sm-2 control-label">PO/Job#</label>
                                    <div class="col-sm-10"><input type="text" class="form-control" id="jobNumber"></div>
                                </div>
                                <div class="form-group"><label class="col-sm-2 control-label">Revenue</label>
                                     <div class="col-sm-10 input-group m-b"><span class="input-group-addon">$</span> <input type="text" class="form-control" id="revenue"> <span class="input-group-addon">.00</span></div>           
                                </div>                                
                                <div class="form-group"><label class="col-sm-2 control-label">Desired Margin</label>
                                    <div class="col-sm-10"><input type="text" class="form-control" id="desiredMargin"></div>
                                </div>                                           
                            	<div class="form-group" id="data_1">
                            	    <label class="col-sm-2 control-label">Simple data input format</label>
                            	    <div class="col-sm-10 input-group date">
                            	        <span class="input-group-addon"><i class="fa fa-calendar"></i></span><input type="text" class="form-control" value="03/04/2014">
                                	</div>
                            	</div>                                         

                                <div class="hr-line-dashed"></div>
                                <div class="form-group"><label class="col-sm-2 control-label">Street</label>
                                    <div class="col-sm-10"><input type="text" class="form-control" id="street1"></div>
                                </div> 
                                <div class="form-group"><label class="col-sm-2 control-label"></label>
                                    <div class="col-sm-10"><input type="text" class="form-control" id="street2"></div>
                                </div>     
                                <div class="form-group"><label class="col-sm-2 control-label">City</label>
                                    <div class="col-sm-10"><input type="text" class="form-control" id="city"></div>
                                </div>     
                                <div class="form-group"><label class="col-sm-2 control-label">State</label>
                                    <div class="col-sm-10"><input type="text" class="form-control" id="state"></div>
                                </div>
                                <div class="form-group"><label class="col-sm-2 control-label">Zip Code</label>
                                    <div class="col-sm-10"><input type="text" class="form-control" id="zipCode"></div>
                                </div>                                


			       			</form>
                        </div>
                    </div>
                </div>
            </div>
            <input class = "btn btn-lg btn-primary btn-block" id="submitJob" value = "Create Job" />
        </div>

        </div>
        </div>

