<!doctype html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />

	<title>Hourwise</title>

	<meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />

    <link rel="stylesheet" href="/styles/bootstrap.min.css">
    <link rel="stylesheet" href="/styles/owl.theme.css">
    <link rel="stylesheet" href="/styles/owl.carousel.css">
    <link rel="stylesheet" href="/styles/jquery.vegas.min.css">
    <link rel="stylesheet" href="/styles/animate.min.css">
    <!-- <link rel="stylesheet" href="/styles/importer.css"> -->


    <link rel="stylesheet" href="/assets/icon-fonts/styles.css"> 
    <link rel="stylesheet" href="/styles/pixeden-icons.css">
    <link href="/css/plugins/switchery/switchery.css" rel="stylesheet">

    <!-- CUSTOM STYLES -->
    <link rel="stylesheet" href="/styles/styles.css">
    <link rel="stylesheet" href="/styles/responsive.css"> 

    <style>
      .blue-bg {background: #879F48;}
      .btn-success {background-color: #879F48; border-color: black;}
      .btn-lg {border-radius: 60px;}
      .form-group {padding: 4%;}
      .form-control1 {
        background-color: #FFFFFF;
        background-image: none;
        border: 1px solid #e5e6e7;
        border-radius: 1px;
        color: inherit;
        display: block;
        padding: 6px 12px;
        transition: border-color 0.15s ease-in-out 0s, box-shadow 0.15s ease-in-out 0s;
        width: 100%;
        font-size: 14px;
      }
      .form-control1:focus {
        border-color: #1ab394!important;
      }
    </style>
  <!-- <link href="http://netdna.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.css" rel="stylesheet"> -->
  
</head>

<body>
<div class="image-container" style="background-image: url('/images/backgrounds/bg3.jpg')">
  <div class="container">
  <div class="row" style="margin-top: 2%;">
    <section class="packages" style="background: none; padding-top: 0px!important;">

    <div class="col-lg-4 col-sm-4">
      <!-- <div class = "col-xs-12"> -->
        <div class="package wow fadeInLeft animated" data-wow-offset="30" data-wow-duration="1.5s" data-wow-delay="0.15s">
            <div class="package-header blue-bg" style="background: #879F48!important">
                <h5>Just a bit of info first...</h5>
            </div>
        
            <form class="form" style="margin-top: 5%;">
              <label id="enterEmail" style="color: red; display: none; padding: 1%;">Please enter your email and name before continuing...</label> 
              <div class="form-group">
                  <input type="text" class="form-control1" name="email" placeholder="Email" id="emailInput" >
              </div>
              <div class="form-group">
                  <input type="text" class="form-control1" name="name" placeholder="Name" id="nameInput">
              </div>
              <!-- <div class = "form-group">
                  <input type="password" class="form-control" name="password" placeholder="Password">
              </div> -->
            </form>
            <div class="row">
              <div class="col-offset-8"><a href="" id="referrer">Referral?</a></div>
            </div>
            <div class="row" id="referrerForm" style="display: none;">
              <div class = "col-xs-12">

              <% if (user.email != 'none') { %>
                <div class="form-group">
                  <input type="text" class="form-control1" id="referrerEmail" placeholder="<%= user.email %>" value="<%= user.email %>" disabled>
                </div>
              <% } else { %>
                <div class="form-group">
                  <input type="text" class="form-control1" id="referrerEmail" placeholder="Referrer email">
                </div>
              <% } %>

              </div>
        </div>

      </div>
      
    <!-- </div> -->
    </section>
    <div class="col-lg-8 col-sm-8">
      <img src="/images/logo_multinbg.png" style="width: 100%;">
    </div>
  </div>
  </div>
    <section class="packages" id="pricingtable" style="background: none; padding-top: 0px!important;">
    <div class="container">
        
        <!-- SECTION HEADER -->
        
        <h2 class="white-text">Services</h2>
            
            <!-- SHORT DESCRIPTION ABOUT THE SECTION -->
            <h6 class="white-text">
                Select the services you're interested in. 
            </h6>
        <!-- / END SECTION HEADER -->
        
        <!-- PACKAGES -->
        <div class="row">
            
            <!-- FIRST PACKAGE -->
            <div class="col-lg-4 col-sm-4">
                <div class="package wow fadeInLeft animated" data-wow-offset="30" data-wow-duration="1.5s" data-wow-delay="0.15s">
                    <div class="package-header blue-bg" id="leadGenHeader">
                        <h5><div align="left" style="margin-left: 5%;"><input type="checkbox" class="js-switch" checked />&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Lead Generation</div></h5>
                    </div>
                
                    <ul>
                        <!-- <li><i class="icon-check"></i>50 minuts of concierge time</li> -->
                        <li><i class="icon-check"></i>PRODUCE LEADS</li>
                        <li><i class="icon-check"></i>CONFIRM WORK</li>
                        <li><i class="icon-check"></i>SCHEDULE MEETING</li>
                                       
                    </ul>
                    <div style="padding: 11px;"></div>
                    <a data-toggle="modal" href="#expandLeadGen"><img alt="image" class="img-circle" src="/images/leadGen.png" style="width: 200px; height: 200px;"></a>
                </div>
            </div> <!-- / END FIRST PACKAGE -->

                    <!-- Second PACKAGE -->
            <div class="col-lg-4 col-sm-4">
                <div class="package wow fadeInLeft animated" data-wow-offset="30" data-wow-duration="1.5s" data-wow-delay="0.15s">
                    <div class="package-header blue-bg" id="pipelineHeader">
                        <h5><div align="left" style="margin-left: 5%;"><input type="checkbox" class="js-switch" checked />&nbsp;&nbsp;&nbsp;Pipeline Management</div></h5>
                    </div>
                    
                    <ul>
                        <!-- <li><i class="icon-check"></i>50 minuts of concierge time</li> -->
                        <li><i class="icon-check"></i>CENTRALIZE LEADS</li>
                        <li><i class="icon-check"></i>FOLLOW UP WITH CUSTOMERS</li>
                        <li><i class="icon-check"></i>REPORT ON LEADS</li>
                                         
                    </ul>
                    <div style="padding: 11px;"></div>
                    <a data-toggle="modal" href="#expandPipeline"><img alt="image" class="img-circle" src="/images/pipeline.png" style="width: 200px; height: 200px;"></a>
                </div>
            </div>
            <!-- / END Second PACKAGE -->

                    <!-- Third PACKAGE -->
            <div class="col-lg-4 col-sm-4">
                <div class="package wow fadeInLeft animated" data-wow-offset="30" data-wow-duration="1.5s" data-wow-delay="0.15s">
                    <div class="package-header blue-bg" id="documentHeader">
                        <h5><div align="left" style="margin-left: 5%;"><input type="checkbox" class="js-switch" checked />&nbsp;&nbsp;&nbsp;Document Management</div></h5>
                    </div>
                    
                    <ul>
                        <!-- <li><i class="icon-check"></i>50 minuts of concierge time</li> -->
                        <li><i class="icon-check"></i>GENERATE PROFESSIONAL PROPOSALS/INVOICES</li>
                        <li><i class="icon-check"></i>TRACK STATUS OF DOCUMENTS</li>
                        <li><i class="icon-check"></i>FOLLOW UP WITH CUSTOMERS</li>
                                         
                    </ul>

                    <a data-toggle="modal" href="#expandDocument"><img alt="image" class="img-circle" src="/images/docCreation.png" style="width: 200px; height: 200px;"></a>

                </div>
                
                <input type='button' class='btn btn-submit btn-fill btn-success btn-wd btn-lg' style="width: 100%;" name='next' value='Next' id="nextButton"/>
            </div>
            <!-- / END Third PACKAGE -->


            <!-- / END PACKAGES -->
        
    </div> <!--END CONTAINER  -->
  </section> <!-- END PACKAGES SECTION -->

</div>
<div class="modal fade" id="expandPipeline" tabindex="-1" role="dialog">
    <div class="modal-dialog">
    
      <!-- Modal content-->
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal">&times;</button>
          <h4>Pipeline Management</h4>
        </div>
        <div class="modal-body" style="overflow-y: auto; max-height: 500px;">
          <p><img alt="image" style="width: 100%;" src="/images/pipeline.png"></p>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
        </div>
      </div>
      
    </div>
  </div>

  <div class="modal fade" id="expandLeadGen" tabindex="-1" role="dialog">
    <div class="modal-dialog">
    
      <!-- Modal content-->
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal">&times;</button>
          <h4>Lead Generation</h4>
        </div>
        <div class="modal-body" style="overflow-y: auto; max-height: 500px;">
          <p><img alt="image" style="width: 100%;" src="/images/leadGen.png"></p>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
        </div>
      </div>
      
    </div>
  </div>

  <div class="modal fade" id="expandDocument" tabindex="-1" role="dialog">
    <div class="modal-dialog">
    
      <!-- Modal content-->
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal">&times;</button>
          <h4>Document Management</h4>
        </div>
        <div class="modal-body" style="overflow-y: auto; max-height: 500px;">
          <p><img alt="image" style="width: 100%;" src="/images/docCreation.png"></p>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
        </div>
      </div>
      
    </div>
  </div>
  

</body>

  <script src="/js/jquery-2.1.1.js" type="text/javascript"></script>
	<script src="/js/bootstrap.min.js" type="text/javascript"></script>
		
	<!--   plugins 	 -->
	<script src="/js/jquery.bootstrap.wizard.js" type="text/javascript"></script>
  <script src="/js/plugins/switchery/switchery.js"></script>

	<script src="/js/firstStep.js"></script>
	
</html>