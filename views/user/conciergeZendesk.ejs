<div class="panel-heading">
    <div class="panel-title m-b-md"><h4></h4></div>
    <div class="panel-options">

        <ul class="nav nav-tabs">
            <li class=""><a data-toggle="tab" href="#tab-1" aria-expanded="false">Unsolved (<%= unsolvedTickets.length %>)</a></li>
            <li class="active"><a data-toggle="tab" href="#tab-2" aria-expanded="true">Unassigned (<%= unassignedTickets.length %>)</a></li>
        </ul>
    </div>
</div>

<div class="panel-body">

    <div class="tab-content">
        <div id="tab-1" class="tab-pane">
            <div class="row">
                <div class="col-lg-12">
                    <div class="wrapper wrapper-content animated fadeInUp">
                        <table class="table">
                            <th>Subject</th>
                            <th>Requester</th>
                            <th>Request Date</th>
                            <th>Ticket Type</th>
                            <th>Status</th>
                            <th>Link</th>
                            <% _.each(unsolvedTickets, function(ticket) { %>
                                <tr>
                                    <td><%= ticket.subject %></td>
                                    <td><%= ticket.requester_id %></td>
                                    <td><%= ticket.created_at %></td>
                                    <td><%= ticket.via.channel %></td>
                                    <td><%= ticket.status %></td>
                                    <td><a href="<%= ticket.url %>">Go to ticket</a></td>
                                </tr>
                            <% }) %>
                         </table>
                    </div>
                </div>
            </div>
        </div>

        <div id="tab-2" class="tab-pane active">
            <div class="row">
                <div class="col-lg-12">
                    <div class="wrapper wrapper-content animated fadeInUp">
                    	<table class="table">
							<th>Subject</th>
							<th>Requester</th>
							<th>Request Date</th>
							<th>Ticket Type</th>
							<th>Link</th>
							<% _.each(unassignedTickets, function(ticket) { %>
								<tr>
									<td><%= ticket.subject %></td>
									<td><%= ticket.requester_id %></td>
									<td><%= ticket.created_at %></td>
									<td><%= ticket.via.channel %></td>
									<td><a href="<%= ticket.url %>">Go to ticket</a></td>
								</tr>
							<% }) %>
                         </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


