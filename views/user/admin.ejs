
        <div class="row">
            <div class="col-lg-12">
                <div class="wrapper wrapper-content animated fadeInUp">

                    <div class="ibox">
                        <div class="ibox-title">
                            <h5>Companies List</h5>
                            <div class="ibox-tools">
                                <a href="/company/new" class="btn btn-primary btn-xs">Create new Company</a>
                            </div>
                        </div>
                        <div class="ibox-content">
                            <div class="row m-b-sm m-t-sm">
                                <div class="col-md-1">
                                    <button type="button" id="loading-example-btn" class="btn btn-white btn-sm" ><i class="fa fa-refresh"></i> Refresh</button>
                                </div>
                                <div class="col-md-11">
                                    <div class="input-group"><input type="text" placeholder="Search" class="input-sm form-control"> <span class="input-group-btn">
                                        <button type="button" class="btn btn-sm btn-primary"> Go!</button> </span></div>
                                    </div>
                                </div>

                                <div class="project-list">

                                    <table class="table table-hover">
                                        <tbody>
                                            <% _.each(companies, function(company){ %>
                                            <tr data-id = "<% company.id %>" data-model = "company">
                                                <td class="project-status">
                                                    <span class="label label-primary"><%= company.accountSettings.plan %></span>
                                                </td>
                                                <td class="project-title">
                                                    <a href="/company/<%= company.id %>"><%= company.name%></a>
                                                    <br/>
                                                    <small>Since <%= company.createdAt%></small>
                                                </td>
                                                <td class="project-title">
                                                    <a href="#"><%= company.primaryIndustry%></a>   
                                                </td>                                                
                                                <td class="project-completion">
                                                    <small>Engagement: 48%</small>
                                                    <div class="progress progress-mini">
                                                        <div style="width: 48%;" class="progress-bar"></div>
                                                    </div>
                                                </td>
                                                <td class="project-people">
                                                    <a href="/company/<%= company.id %>#users"><%= company.employees.length%> users </a>
                                                </td>
                                                <td class="project-actions">
                                                    <a href="/company/show/<%= company.id %>" class="btn btn-white btn-sm"><i class="fa fa-folder"></i> View </a>
                                                    <a href="/company/edit/<%= company.id %>#users" class="btn btn-white btn-sm"><i class="fa fa-pencil"></i> Edit </a>
                                                </td>
                                            </tr>
                                            
                                            <% }) %>                                            
                                          
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="footer">

                <div>
                    <strong>Copyright</strong> Hourwise &copy; 2014-2015
                </div>
            </div>


